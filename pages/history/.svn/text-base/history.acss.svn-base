page{
	background: #F6F6F6;
	border: 1px solid rgba(0,0,0,0);
}
.clear{
	clear: both;
}
.history{
	margin-bottom: 40rpx;
}
.historyList{
	margin-top: 32rpx;
}
.listTime{
	width:158rpx;
	border-radius: 6rpx;
	background:rgba(25,31,37,0.2);
	color: #fff;
	font-size:24rpx;
	font-weight:400;
	text-align: center;
	line-height: 48rpx;
	margin: 0 auto;
}
.historyCard{
	width: 622rpx;
	/* height: 364rpx; */
	background: #fff;
	border-radius: 6rpx;
	margin: 40rpx auto 0;
	padding: 32rpx 32rpx 16rpx 32rpx;
	font-size: 28rpx;
}
.cardList{
	line-height: 40rpx;
	margin-bottom: 12rpx;
}
.labeUse{
	color:rgba(25,31,37,0.56);
	float: left;
}
.cardWeek{
	float: right;
	border: 2rpx solid #1AABF8;
	border-radius: 4rpx;
	font-size: 20rpx;
	font-weight: 500;
	text-align: center;
	color: #1AABF8;
	width: 78rpx;
}
.midText{
	display: inline-block;
	width: 470rpx;
	float: left;
}
.line{
	margin-top: 32rpx;
	height: 2rpx;
	background:rgba(25,31,37,0.08);
}
.deleteBtn{
	float: right
}
.botDelete{
	margin-top: 20rpx;
	height: 64rpx;
}
.meetingStatus{
	color:rgba(25,31,37,0.56);
	font-size:24rpx;
	line-height: 64rpx;
}
.deleteBtn{
	border: 1px solid rgba(25,31,37,.12);
	border-radius: 6rpx;
	font-weight: 500;
	color: rgba(25,31,37,0.56);
	width:112rpx;
	padding: 10rpx 0;
	font-size: 28rpx;
	text-align: center;
}
.nohistory{
	margin: 293rpx auto 0;
	text-align: center;
}
.noDataPic{
	width: 144rpx;
	height: 144rpx;
	margin: 0 auto 48rpx;
}
.noDataPic image {
	width: 100%;
	height: 100%;
}
.nohistoryText{
	font-size: 34rpx;
	color:rgba(25,31,37,0.4);
}