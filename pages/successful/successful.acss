page{
	background: #f6f6f6;
}
.topIcon{
	background: #fff;
	height: 334rpx;
	border:1px solid rgba(0,0,0,0);
	margin-bottom: 60rpx;
}
.succIcon{
	width: 128rpx;
	height: 128rpx;
	margin: 60rpx auto 40rpx;
}
.succIcon image{
	width: 100%;
	height: 100%;
}
.successText{
	color: #191F25;
	text-align: center;
	font-size: 40rpx;
	font-weight: 400;
	margin-bottom: 72rpx;
}
.searchDetail{
	width: 576rpx;
	border-radius: 6rpx;
	background: #1AABF8;
	font-size: #fff;
	font-size: 34rpx;
	font-weight: 500;
	margin: 0 auto;
	text-align: center;
	color: #fff;
	line-height: 88rpx;
	margin-bottom: 32rpx;
}
.backList{
	width: 576rpx;
	border-radius: 6rpx;
	border:1px solid rgba(25,31,37,0.12);
	text-align: center;
	background: #fff;
	line-height: 88rpx;
	font-weight: 500;
	font-size: 34rpx;
	color:rgba(25,31,37,0.56);
	margin: 0 auto;
}