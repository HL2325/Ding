page{
	background: #f6f6f6;
}
.clear{
	clear: both;
}
.person_detail{
	border: 1px solid rgba(0,0,0,0);
}
.p_dMain{
	margin-top: 32rpx;
}
.p_detailList{
	padding: 16rpx 32rpx;
	border-bottom: 1px solid rgba(25,31,37,0.08);
	background: #fff;
	box-sizing: border-box;
	line-height: 66rpx;
}
.p_dPic{
	width: 64rpx;
	height: 64rpx;
	border-radius: 50%;
	background: #1AABF8;
	color: #fff;
	font-size: 28rpx;
	line-height: 64rpx;
	text-align: center;
	float: left;
}
.p_dName{
	font-size:30rpx;
	color:rgba(25,31,37,0.56);
	margin-left: 32rpx;
	float: left;
}
.dingPic{
	float: right;
	width: 48rpx;
	height: 48rpx;
	margin-top: 13rpx;
}
.dingPic image{
	width: 100%;
	height: 100%;
}